<feature
      id="org.codehaus.groovy16.feature"
      label="%featureName"
      version="2.0.0.qualifier"
      provider-name="%providerName">

   <description url="%descriptionURL">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%license.html">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://dist.codehaus.org/groovy/distributions/greclipse/snapshot/e3.5/"/>
   </url>

   <requires>
      <import plugin="org.apache.ant"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.equinox.p2.metadata" version="1.0.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.p2.metadata.generator" version="1.0.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.p2.metadata.repository" version="1.0.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.p2.engine" version="1.0.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.p2.core" version="1.0.100" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.codehaus.groovy"
         download-size="0"
         install-size="0"
         version="1.6.5.qualifier"/>

</feature>
