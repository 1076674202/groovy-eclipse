<feature
      id="org.codehaus.groovy.alltests.feature"
      label="%featureName"
      version="2.1.2.qualifier"
      provider-name="%Codehaus.org">

   <description url="%descriptionURL">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.codehaus.groovy" version="1.6.7" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.6.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.groovy.core"/>
      <import plugin="org.eclipse.core.resources" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.codehaus.groovy.eclipse.codebrowsing" version="2.1.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui" version="3.6.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.ui" version="3.6.1" match="greaterOrEqual"/>
      <import plugin="org.codehaus.groovy.eclipse.codeassist.completion"/>
      <import plugin="org.codehaus.groovy.eclipse.ui" version="2.1.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface.text" version="3.6.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.6.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.codehaus.groovy.eclipse.dsl"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
