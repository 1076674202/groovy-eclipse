      label="%featureName"
      version="2.9.2.qualifier"
      provider-name="%providerName">

   <description url="%descriptionURL">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.codehaus.groovy" version="1.8.9" match="greaterOrEqual"/>
      <import plugin="org.codehaus.groovy.eclipse.core" version="2.9.0" match="greaterOrEqual"/>
      <import feature="org.codehaus.groovy.eclipse.feature" version="2.9.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.7.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.groovy.core" version="2.9.0" match="greaterOrEqual"/>
      <!--import feature="org.eclipse.m2e.feature" version="1.0.0" match="greaterOrEqual"/-->
   </requires>

   <plugin
         id="org.codehaus.groovy.m2eclipse"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"
   />

</feature>
